@incollection{tobler_movement_1993,
	address = {Berlin, Heidelberg},
	title = {Movement {Takes} {Time}: {A} {Note} on the {Time} of {Movement}},
	isbn = {978-3-642-78094-3},
	shorttitle = {Movement {Takes} {Time}},
	url = {https://doi.org/10.1007/978-3-642-78094-3_5},
	abstract = {Continuous transportation modelling was introduced by Martin Beckmann in 1952, and continues in some of his recent books with Tonu Puu. My own recent work has also dealt with this topic and with spatial interaction models in both continuous and discrete form, especially as related to migration. The general class of models here includes the PP/D gravity model as used by Zipf, Stewart, Warntz, Isard, and others: the entropy model introduced by Alan Wilson; the Hitchcock-Koopmans-Kantorovitch linear transportation problem, the quadratic transportation problem, and the spatial price equilibrium model described by Samuelson and later workers. In all of these problems I have constantly encountered two difficulties.},
	language = {en},
	urldate = {2020-05-13},
	booktitle = {Structure and {Change} in the {Space} {Economy}: {Festschrift} in {Honor} of {Martin} {J}. {Beckmann}},
	publisher = {Springer},
	author = {Tobler, Waldo},
	editor = {Lakshmanan, T. R. and Nijkamp, Peter},
	year = {1993},
	keywords = {CC-Mouvements, Discrete Choice Analysis, Employee Number, LocCC-CDMV, LocCC-RRWT, Spatial Interaction Model, Transportation Problem, Urban Traffic},
	pages = {61--65},
	annote = {Résumé C. Reymond-Cauvin, 16/06/2020
Attention, seules les 2 premières pages sont disponibles à ce jour.
De nombreux modèles d'interaction spatiale sous forme continue ou discrèe existent actuellement. Mais deux difficultés empêchent de les utiliser dans certains cas :
1. Ces formulations correspondent à des modèles pour des données de comportements agrégés, alors que maintenant on dispose de données au niveau des individus avec leurs coordonnées géographiques et des indications liées au temps. Comment intégrer ces données dans les modèles sans agrégation ?
2. Les modèles d'Interaction spatiale décrivent des situations d'équilibre et n'informent pas sur les chemins empruntés.
 
Tobler propose un modèle ET LA SUITE N'A PAS ETE DISPONIBLE...},
}

